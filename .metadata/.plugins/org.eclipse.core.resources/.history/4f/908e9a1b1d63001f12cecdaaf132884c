package Elementos;

import org.openqa.selenium.By;

public class ElementosCriarRegistro {

	
		private By campoElements = By
				.cssSelector("#app > div > div > div.home-body > div > div:nth-child(1)");
		private By submenuWebTables = By.cssSelector("#item-3 > span");
		private By novoRegistroAdd = By.id("addNewRecordButton");
		private By FirstName = By.id("firstName");
		private By LastName = By.id("lastName");
		private By Email = By.id("userEmail");
		private By Age = By.id("age");	
		private By salary = By.id("salary");
		private By department = By.id("department");
		private By submitRegistro = By.cssSelector("#submit");
		private By btEdit = By.cssSelector("#edit-record-4 > svg > path");
		private By elementEdit = By.cssSelector("#app > div > div > div > div.col-12.mt-4.col-md-6 > div.web-tables-wrapper > div.ReactTable.-striped.-highlight > div.rt-table > div.rt-tbody > div:nth-child(4) > div > div:nth-child(3)");
		private By btDelete4 = By.cssSelector("#delete-record-4 > svg");
		private By btDelete5 = By.cssSelector("#delete-record-5 > svg");
		private By btDelete6 = By.cssSelector("#delete-record-6 > svg");
		private By btDelete7 = By.cssSelector("#delete-record-7 > svg");
		private By btDelete8 = By.cssSelector("#delete-record-8 > svg");
		private By btDelete9 = By.cssSelector("#delete-record-9 > svg");
		private By btDelete10 = By.cssSelector("#delete-record-10 > svg");
		private By btDelete11 = By.cssSelector("#delete-record-11 > svg");
		
		
		public By getBtDelete4() {
			return btDelete4;
		}
		public By getBtDelete5() {
			return btDelete5;
		}
		public By getBtDelete6() {
			return btDelete6;
		}
		public By getBtDelete7() {
			return btDelete7;
		}
		public By getBtDelete8() {
			return btDelete8;
		}
		public By getBtDelete9() {
			return btDelete9;
		}
		public By getBtDelete10() {
			return btDelete10;
		}
		public By getBtDelete11() {
			return btDelete11;
		}
		public By getBtDelete12() {
			return btDelete12;
		}
		private By btDelete12 = By.cssSelector("#delete-record-12 > svg");
		
		public By getCampoElements() {
			return campoElements;
		}
		public By getSubmenuWebTables() {
			return submenuWebTables;
		}
		public By getNovoRegistroAdd() {
			return novoRegistroAdd;
		}
		public By getFirstName() {
			return FirstName;
		}
		public By getLastName() {
			return LastName;
		}
		public By getEmail() {
			return Email;
		}
		public By getAge() {
			return Age;
		}
		public By getSalary() {
			return salary;
		}
		public By getDepartment() {
			return department;
		}
		public By getSubmitRegistro() {
			return submitRegistro;
		}
		public By getBtEdit() {
			return btEdit;
		}
		public By getElementEdit() {
			return elementEdit;
		}
		public By getBtDelete() {
			return btDelete;
		}
	
}
