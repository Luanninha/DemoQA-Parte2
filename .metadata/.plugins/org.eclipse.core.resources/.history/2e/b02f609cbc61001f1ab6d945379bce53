    private By campoDateOfBirthInput = By.id("dateOfBirthInput");
    private By selecaoMes = By.cssSelector(".react-datepicker__month-select");
    private By selecaoAno = By.cssSelector(".react-datepicker__year-select");
    private By selecaoDia = By.cssSelector(".react-datepicker__day--001");
    private By campoSubjects = By.cssSelector(".subjects-auto-complete__value-container");
    private By opcaoSports = By.cssSelector(".custom-control-label");
    private By campoUploadPicture = By.id("uploadPicture");
    private By campoCurrentAddress = By.id("currentAddress");
    private By selecaoEstado = By.id("state");
    private By selecaoCidade = By.id("city");
    private By botaoSubmit = By.id("submit");
    private By botaoFecharPopup = By.id("closeLargeModal");
      
//Validar mensagem 
private By campoAlertFrameWindow = By.cssSelector("#app > div > div > div.home-body > div > div:nth-child(3) > div > div.card-body > h5");
private By submenuBrowserWindow = By.cssSelector(null);
private By botaoNewWindow = By.cssSelector(null);
