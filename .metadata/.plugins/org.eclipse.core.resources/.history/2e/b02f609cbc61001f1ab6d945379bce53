package Elementos;

import org.openqa.selenium.By;

public class Elementos {

	//Criar registro
	private By campoElements = By.cssSelector(null);
	 private By submenuWebTables = By.cssSelector(null);
	 private By novoRegistroNome = By.cssSelector(null);
	 private By novoRegistroAge = By.cssSelector(null);
	 private By novoRegistroEmail = By.cssSelector(null);
	 private By registroAtualizado = By.cssSelector(null);

