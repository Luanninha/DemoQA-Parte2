package Elementos;

import org.openqa.selenium.By;

public class ElementoValidarProgress {


	
		private By campoWidget = By.cssSelector(
				"#app > div > div > div.home-body > div > div:nth-child(4) > div > div.avatar.mx-auto.white > svg > path");
		private By campoProgressBar = By.cssSelector("#item-4 > span");
		private By btStart = By.cssSelector("#startStopButton");
		private By ProgressBar = By.id("progressBar");
		
		
		public By getCampoWidget() {
			return campoWidget;
		}
		public By getCampoProgressBar() {
			return campoProgressBar;
		}
		public By getBtStart() {
			return btStart;
		}
		public By getProgressBar() {
			return ProgressBar;
		}
		

	}